{
	"name": "cs410-chrome-extension",
	"version": "0.1",
	"description": "Description of extension",
	"manifest_version": 2,
	"permissions": ["activeTab", "storage", "tabs",  "unlimitedStorage","<all_urls>"],
	"browser_action": {         
      "default_popup": "popup.html"
    },

	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["main.js"]
		}
	]

}